                    <!-- Slider Web Part Zone -->
                    <div class="row full-width">
                        <div data-name="SliderWebPartZone">
                            
                            
                            <div>
                                <div id="sliderWebPartZone" class="AspNet-WebPartZone-Vertical">

			</div>
		
