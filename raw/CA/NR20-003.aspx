                            pageURL = results[i].Cells.results[6].Value;
                            resultHTML += "<li>"
                            resultHTML += "<a href=" + pageURL + ">" + showLessMoreText(title, 3) + "</a>"
                            resultHTML += "</li>"
                            if (((itemCount % columnCount) == 0) || (i == resultCount - 1)) {
